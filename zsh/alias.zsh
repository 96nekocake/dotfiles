## 省略形
alias cl='clear'
alias c='clear'
alias h='history'
alias to='touch'
alias vi='vim'
alias :q='exit'
alias re='source ~/.zshrc'
alias ...='cd ../..'
alias ....='cd ../../..'

# 新規コマンド / プラグインコマンド
alias ls='eza'
alias ll='eza -ahl --git'
alias cat='bat'
alias vim='nvim'
alias see='hub see'
alias clock='tty-clock -scC 4'

